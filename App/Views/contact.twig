    <script>
        function initMap() {
            var uluru1 = {lat: {{elements.ln_contact_info.ln_lat}}, lng: {{elements.ln_contact_info.ln_lang}}};
            var map1 = new google.maps.Map(document.getElementById('londonmap'), {
                zoom: 14,
                center: uluru1
            });
            var marker1 = new google.maps.Marker({
                position: uluru1,
                map: map1
            });

            var uluru2 = {lat: {{elements.contact_info.lat}}, lng: {{elements.contact_info.lang}}};
            var map2 = new google.maps.Map(document.getElementById('centralmap'), {
                zoom: 14,
