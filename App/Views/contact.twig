    <section class="contact-form">
        <div class="container">
            <div class="row">
                <div class="col-sm-5">
                    <div class="title">Contact Us Form:</div>
                    <div class="request">You can contact us through this form</div>
                    <div class="request">Please enter your contact information and request</div>
                    <div class="request">Our colleagues will contact you as soon as possible</div>
                    <div class="image ">
                        <img src="/images/telecontact.png" class="img-responsive center-block">
                    </div>
                </div>
                <div class="col-sm-7">
                    <form>
                        <div class="form-group">
                            <input type="text" name="name" class="form-control input-control" id="exampleInputEmail1"
                                   placeholder="Name/Surname">
                        </div>
                        <div class="form-group">
                            <input type="text" name="telephone" class="form-control input-control" id="exampleInputEmail1"
                                   placeholder="Tel">
                        </div>
                        <div class="form-group">
                            <input type="email" name="email" class="form-control input-control" id="exampleInputEmail1"
                                   placeholder="Email">
                        </div>
                        <div class="form-group">
                            <select name="department" id="" class="form-control">
                                <option selected="selected">Department</option>
                                {#<option value="dooor">oops</option>#}
                            </select>
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" rows="5" placeholder="Message"></textarea>
                        </div>
                        <div class="row">
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <input type="text" name="code" class="form-control text-center" id="exampleInputEmail1"
                                           placeholder="Random">                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                    <input type="text" name="code" class="form-control text-center" id="exampleInputEmail1"
                                           placeholder="Type the code">
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="form-group">
                                <a href="" class="btn btn-send pull-right btn-block">SEND</a>
                                </div>
                            </div>
                        </div>
                        {#<div class="form-group">#}
                            {#<button type="submit" class="btn btn-send col-sm-3">SEND</button>#}
                            {#<input type="text" class="form-control text-center col-sm-3" name="code" placeholder="Type the Code">#}
                        {#</div>#}

                    </form>
                </div>
            </div><!--row-->
        </div><!--container-->
    </section>

